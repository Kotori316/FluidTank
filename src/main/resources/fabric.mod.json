  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric": ">=0.55.3+1.19",
    "minecraft": "1.19.x",
    "fabric-language-scala": ">=0.3.0"
  },
  "custom": {
    "waila:plugins": [
      {
        "id": "fluidtank:wthit_plugin",
        "initializer": "com.kotori316.fluidtank.integration.wthit.WthitTankWailaPlugin"
