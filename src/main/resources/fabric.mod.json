{
  "schemaVersion": 1,
  "id": "fluidtank",
  "version": "${version}",
  "name": "Fluid Tank",
  "description": "FluidTank",
  "authors": [
    "Kotori316"
  ],
  "contact": {
    "homepage": "https://github.com/Kotori316",
    "sources": "https://github.com/Kotori316/FluidTank",
    "issues": "https://github.com/Kotori316/FluidTank/issues"
  },
  "license": "All rights reserved",
  "icon": "assets/fluidtank/textures/tank_big.png",
